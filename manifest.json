{
  "manifest_version": 2,
  "name": "Austerity",
  "short_name": "Austerity",
  "description": "Port of Dominion",
  

  "version": "0.0.1.0",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },
  
  "permissions": [{
    "socket": [
      "tcp-connect",
      "tcp-listen"
    ]
  },
  "storage",
  "unlimitedStorage"
  ],
      
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  }
}

{
  "name": "austerity",
  "version": "0.0.0",
  "description": "Gameboard web server",
  "main": "server/app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "cli": "node cli/cli.js",
    "start": "node server/app.js",
    "pretest": "eslint --fix server client *.js && tsc -p jsconfig.json",
    "test": "jasmine *spec.js",
    "watch": "supervisor --poll-interval 2000 --quiet --ignore client,.git,node_modules server/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/darthwalsh/Austerity.git"
  },
  "author": "Carl Walsh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darthwalsh/Austerity/issues"
  },
  "homepage": "https://github.com/darthwalsh/Austerity",
  "devDependencies": {
    "@types/csv-parse": "1.1.12",
    "@types/express": "4.16.1",
    "@types/jasmine": "3.3.12",
    "@types/node": "11.13.0",
    "@types/ws": "6.0.1",
    "eslint": "5.16.0",
    "eslint-config-google": "0.12.0",
    "jasmine": "3.4.0",
    "jasmine-reporters": "2.3.2",
    "supervisor": "0.12.0",
    "typescript": "3.4.1"
  },
  "dependencies": {
    "csv-parse": "^4.3.3",
    "express": "^4.16.4",
    "ws": "^6.1.2"
  }
